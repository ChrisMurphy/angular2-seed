{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "Starting point for a blank ng2 application",
  "main": "index.js",
  "scripts": {
    "start": "live-server --port=9090 ./src",
    "postinstall": "jspm install && tsd reinstall -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/StepChangeCharity/angular2-seed.git"
  },
  "author": "Chris Murphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StepChangeCharity/angular2-seed/issues"
  },
  "homepage": "https://github.com/StepChangeCharity/angular2-seed",
  "devDependencies": {
    "jspm": "^0.16.10"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-alpha.37",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@^0.0.91",
      "zone.js": "npm:zone.js@^0.5.6"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
