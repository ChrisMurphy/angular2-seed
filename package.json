    "frontend",
    "html5",
    "angularjs",
    "angular2",
    "typescript",
    "es6"
  ],
  "homepage": "https://github.com/ChrisMurphy/angular2-seed",
  "devDependencies": {
    "angular2": "^2.0.0-beta.1",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-environments": "^0.1.1",
    "gulp-html-replace": "^1.5.4",
    "gulp-htmlmin": "^1.1.4",
    "gulp-sequence": "^0.4.1",
    "gulp-tslint": "^3.3.0",
