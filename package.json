    "frontend",
    "html5",
    "angularjs",
    "angular2",
    "typescript",
    "es6"
  ],
  "homepage": "https://github.com/ChrisMurphy/angular2-seed",
  "devDependencies": {
    "angular2": "2.0.0-beta.3",
    "del": "^2.0.2",
    "es6-shim": "^0.33.13",
    "gulp": "^3.9.0",
    "gulp-environments": "^0.1.1",
    "gulp-html-replace": "^1.5.4",
    "gulp-htmlmin": "^1.1.4",
