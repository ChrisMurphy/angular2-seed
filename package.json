{
  "name": "ng2-seed",
  "version": "1.0.0",
  "description": "Starting point for a blank ng2 application",
  "main": "./src/app/bootstrap.ts",
  "scripts": {
    "start": "gulp development",
    "development": "gulp development",
    "production": "gulp production",
    "build": "gulp build",
    "postinstall": "jspm install && typings.cmd",
    "setup": "npm install -g live-server typings && npm install",
    "test": "node node_modules/karma/bin/karma start karma.conf.js",
    "coverage": "node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisMurphy/angular2-seed.git"
  },
  "author": "Chris Murphy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChrisMurphy/angular2-seed/issues"
  },
  "keywords": [
    "angular2",
    "typescript",
    "seed",
    "karma",
    "jasmine",
    "coverage"
  ],
  "homepage": "https://github.com/ChrisMurphy/angular2-seed",
  "devDependencies": {
    "angular2": "2.0.0-beta.6",
    "del": "^2.0.2",
    "es6-shim": "^0.33.3",
    "gulp": "^3.9.0",
    "gulp-environments": "^0.1.1",
    "gulp-html-replace": "^1.5.4",
    "gulp-htmlmin": "^1.1.4",
    "gulp-sequence": "^0.4.1",
    "gulp-tslint": "^3.3.0",
    "gulp-tslint-stylish": "^1.1.1",
    "gulp-useref": "^1.3.0",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.16.24",
    "karma": "^0.13.21",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.7",
    "karma-jspm": "^2.0.2",
    "live-server": "^0.8.1",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.5.1",
    "require-dir": "^0.3.0",
    "rxjs": "5.0.0-beta.0",
    "systemjs": "^0.19.21",
    "typescript": "^1.7.5",
    "zone.js": "0.5.14"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "angular2": "npm:angular2@2.0.0-beta.6",
      "es6-shim": "github:es-shims/es6-shim@^0.33.3",
      "reflect-metadata": "npm:reflect-metadata@0.1.2",
      "rxjs": "npm:rxjs@5.0.0-beta.0",
      "zone.js": "npm:zone.js@0.5.14"
    },
    "devDependencies": {
      "ts": "github:frankwallis/plugin-typescript@^3.0.3",
      "typescript": "npm:typescript@^1.7.5"
    }
  }
}
